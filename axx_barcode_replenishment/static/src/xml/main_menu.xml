<templates xml:space="preserve">
    <t t-inherit="stock_barcode.MainMenu" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[hasclass('o_main_menu_buttons')]" position="inside">
            <div class="col">
                <button class="button_replenishment btn btn-block btn-primary mb-4"
                    t-on-click="onclickReplenishment">
                    Nachschub
                </button>
            </div>
            <div class="col">
                <button class="button_replenishment btn btn-block btn-primary mb-4 w-100"
                    t-on-click="onclickEinlagern">
                    Einlagern
                </button>
            </div>
        </xpath>
    </t>
</templates>