<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_product_product_tree_inherit" model="ir.ui.view">
        <field name="name">view.product.product.tree.inherit</field>
        <field name="model">product.product</field>
        <field name="inherit_id" ref="product.product_product_tree_view"/>
        <field name="priority">90</field>
        <field name="arch" type="xml">
            <field name="categ_id" position="after">
                <field name="axx_standard_location_id" optional="hide"/>
                <field name="axx_reserve_location_id" optional="hide"/>
            </field>
        </field>
    </record>

</odoo>